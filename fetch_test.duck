use std::http::{fetch};
use std::io::{println};

fn main() {
    fetch(.GET, "https://google.com")
        .set_body("yolo")
        .put_header(.Authorization, "Bearer 123")
        .put_header(.WWW_Authenticate, "WWW value")
        .put_header_raw("Raw Header", "raw header value")
        .to_string()
        ->println();

    (match fetch(.GET, "https://google.com").send() { .http_error => "error", String @s => s } )->println();
}
