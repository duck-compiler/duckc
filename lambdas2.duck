struct S = {
} impl {
    fn imm_fun() {}

    mut fn needs_mut() {
        std::io::println("HELLO I AM MUT FN");
    }
};

fn inter(d: { needs_mut: mut fn() }) {
    d.needs_mut();
}

fn main() {
    let a = 5;
    a = 10;

    const s = S {};
    inter(s);

    (mut fn () {
        (mut fn() { a = 20; (mut fn() { a = 123; })();})();
        s.imm_fun();
//        s.needs_mut();
    })();
    std::io::println(std::string::from_int(a));
}
