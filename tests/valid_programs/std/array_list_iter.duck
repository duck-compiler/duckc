use std::col::ArrayList;
use std::io::println;

fn main() {
    let list = ArrayList::from_array<String>(["Yo", "I", "am", "in", "the", "list"]);

    for (e in list.iter()) {
        *e->println();
    }

    for (e in list.iter_mut()) {
        *e = f"Modified {*e}";
    }

    for (e in list.iter()) {
        *e->println();
    }

    list.iter().count().to_string()->println();
    let list2 = ArrayList::from_array<String>(["other", "list"]);
    let list2_iter = list2.iter();

    for (pair in list.iter().zip<&String>(&mut list2_iter)) {
        println(f"{*pair.0}, {*pair.1}");
    }

    println("");

    list2_iter = list2.iter();
    for (pair in list.iter().zip<&String>(&mut list2_iter).rev()) {
        println(f"{*pair.0}, {*pair.1}");
    }

    println("");

    for (pair in list.iter().rev()) {
        println(f"{*pair}");
    }

}
