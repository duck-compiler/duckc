use std::sync::{new_channel};

fn main() {
    const chan = new_channel<()>();

    async (fn(){
        std::io::println("Sleeping for 1s and then sending a ()");
        std::task::sleep({seconds: 1});
        chan.send(());
        std::io::println("Sent");
    })();

    std::io::println("Waiting for ()");
    chan.recv_block();
    std::io::println("Got ()");
}
