use go "fmt";

fn main() {
    let x: { a: { b: { c: Int } }, z: Bool } = {
        a: {
            b: {
                c: 10
            }
        },
        z: true
    };

    go {
        fmt.Println(x.Getz())
        fmt.Println(x.Geta().Getb().Getc())
    }

    x.a.b.c = 100;

    go {
        fmt.Println(x.Getz())
        fmt.Println(x.Geta().Getb().Getc())
    }

    x.z = false;

    go {
        fmt.Println(x.Getz())
        fmt.Println(x.Geta().Getb().Getc())
    }

}
