use go "fmt";

module a {
    fn give_from_a() -> String {
        return "from a";
    }

    fn get() -> String {
        return f"A: {give_from_a()}";
    }
}

fn give_string() -> String {return "hallo";}

fn main() {
    let x: String = f"Hallo ich bin aus der Methode {a::get()}";
    go {
        fmt.Println(x)
    }
}
