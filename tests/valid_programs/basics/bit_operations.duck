use std::io::println;

fn main() {
    const z = 1;
    println(z.to_string());
    const z = z << 1;
    println(z.to_string());
    const z = z >> 1;
    println(z.to_string());
    const z = z >> 1;
    println(z.to_string());
    const z = 1;
    println((~z + 1).to_string());
    println((~(~z + 1) + 1).to_string());
    const z = 1 & ~1;
    println(z.to_string());
    const z = 1 | ~1;
    println(z.to_string());
    const z = 1 ^ ~1;
    println(z.to_string());
    const z = 1 ^ 1;
    println(z.to_string());
    println("");
    let z = 1;
    z <<= 1;
    println(z.to_string());
    z <<= 1;
    println(z.to_string());
    z <<= 1;
    println(z.to_string());
    z >>= 1;
    println(z.to_string());
    z >>= 1;
    println(z.to_string());
    z >>= 1;
    println(z.to_string());
}
