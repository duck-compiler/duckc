struct S {
    x: (String[],),
}

fn main() {
    let x: { x: String } = { x: f"a string" };
    x.x
        ->std::string::to_upper()
        ->std::io::println();

    let s: ({x: S},) = ({x: S { x: (["nested"],) }},);
    s.0.x.x.0[0] = "set by assignment";
    s.0.x.x.0[0]->std::io::println();
}
