use std::opt::{Some, Opt};

fn main() {
    let numbers: std::col::ArrayList<Int> = std::col::ArrayList::new<Int>();
    numbers.push(1);
    numbers.push(2);
    numbers.push(3);
    numbers.push(4);
    numbers.push(5);
    let sum: Int = 0;

    std::io::println("Testing while loop with array [1, 2, 3, 4, 5]");
    std::io::println("Expected sum: 15");

    let i: Int = 0;
    let length: Int = 0;
    go {
        length = numbers.len()
    }

    while (i < length) {
        numbers.get(i).if_some(mut fn (value: Int) {
            sum = sum + value
        });
        i = i + 1;
    }

    std::io::println("Actual sum:");
    sum->std::string::from_int()->std::io::println();
}
