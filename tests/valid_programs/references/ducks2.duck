fn main() {
    let d: { x: String[], y: String } = { x: .String["yo", "moin"], y: "String in y" };
    let ref_to_x = &mut d.x;
    std::io::println("before:");
    std::io::debug(d.x);
    std::io::debug(*ref_to_x);
    ref_to_x[0] = "x replaced";
    std::io::println("after:");
    std::io::debug(d.x);
    std::io::debug(*ref_to_x);
}
