struct S = {
    v: Int
} impl {
    mut fn abc() {
        *self = S { v: 20 };
    }

    mut fn replace_and_return() -> &mut S {
        *self = S { v: 123 };
        return self;
    }
};

fn main() {
    let s = S { v: 10 };
    s.v->std::string::from_int()->std::io::println();
    s.abc();
    s.v->std::string::from_int()->std::io::println();
    s.replace_and_return().v->std::string::from_int()->std::io::println();
}
